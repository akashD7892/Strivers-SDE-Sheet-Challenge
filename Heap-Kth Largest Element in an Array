class Solution {
public:
    int findKthLargest(vector<int>& nums, int k) {
        priority_queue <int> gq;
        for( int i=0; i<nums.size(); i++ ){
             gq.push(nums[i]) ;
        }

        int i=1; 
        while( i < k  ){
            gq.pop() ;
            i++;
        }

        int ans = gq.top() ;
        return ans ;
    }
};
